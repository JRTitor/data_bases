SELECT COUNT(Trains_routes.train_routes_id) AS count_of_routes,;
  Trains_routes.station_from_id AS station;
 FROM ;
     trains_routes;
 GROUP BY Trains_routes.station_from_id;
 INTO CURSOR tmp  
 
SELECT count_of_routes, station_name, location;
 FROM tmp, Stations;
 WHERE count_of_routes >= ALL(SELECT count_of_routes FROM tmp);
 	AND station_id = station;
